{
  "manifest_version": 2,
  "name": "Giaoculator",
  "version": "5.0.3",
  "description": "A chrome extension for calculating your GPA.",
  "options_page":  "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [{
      "matches": ["https://*.schoolis.cn/*","*://4.3.2.1/*","*://view.officeapps.live.com/*"],
      "css": ["lib/notyf.min.css"],
      "js": ["lib/notyf.min.js","lib/confetti.browser.min.js","content.js"]
  }],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },"web_accessible_resources": [
    "icon.png",
    "res/disablev2.png",
    "res/disable.png",
    "res/icon.png",
    "res/hideOff.svg",
    "res/hideOn.svg",
    "res/hideOff_Dark.svg",
    "res/hideOn_Dark.svg",
    "res/visOn.svg",
    "res/visOff.svg",
    "res/exp_settings.png",
    "res/tsLogo_W.png",
    "res/tsLogo_D.png",
    "res/download.svg",
    "res/settings.svg",
    "res/green-tick.png",
    "res/iconNormal.png",
    "usr/1.png",
    "usr/1.jpg",
    "usr/1.mp4",
    "usr/1.mov",
    "usr/2.png",
    "usr/2.jpg",
    "usr/2.mp4",
    "usr/2.mov"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ]
}

