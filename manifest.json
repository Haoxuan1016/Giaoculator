{
  "manifest_version": 2,
  "name": "Giaoculator Lite",
  "version": "4.5.6",
  "description": "A chrome extension for calculating your GPA on schoolis. ",
  "options_page":  "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [{
      "matches": ["https://*.schoolis.cn/*"],
      "css": ["lib/notyf.min.css"],
      "js": ["lib/notyf.min.js", "content.js"]
  }],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },"web_accessible_resources": [
    "res/disablev2.png",
    "res/disable.png",
    "res/hideOff.svg",
    "res/hideOn.svg",
    "res/visOn.svg",
    "res/visOff.svg"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ]
}

